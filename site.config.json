{
  "site": {
    "name": "codersinflow",
    "displayName": "Coders in Flow",
    "domain": "codersinflow.com",
    "description": "AI-Powered Pair Programming with Coders In Flow - VSCode Extension for Developers"
  },
  "development": {
    "frontend": {
      "port": 4323
    },
    "backend": {
      "port": 8749
    },
    "mongodb": {
      "port": 27419,
      "database": "codersblog"
    }
  },
  "production": {
    "frontend": {
      "port": 4323
    },
    "backend": {
      "port": 8749
    },
    "mongodb": {
      "database": "codersblog"
    }
  },
  "docker": {
    "prefix": "coders-blog",
    "volumes": {
      "mongodb": "coders_blog_mongodb_data"
    }
  },
  "paths": {
    "uploads": "/var/www/codersinflow.com/uploads",
    "logs": "/var/log/codersinflow.com"
  },
  "theme": {
    "type": "dark",
    "primaryColor": "#3b82f6",
    "accentColor": "#60a5fa"
  },
  "deployment": {
    "server": {
      "host": "ny",
      "user": "root",
      "port": 12222,
      "path": "/var/www/codersinflow.com"
    }
  }
}