# Backend API URL (non-standard port to avoid conflicts)
PUBLIC_API_URL=http://localhost:8749

# JWT Secret for authentication (change this in production!)
JWT_SECRET=your-secret-key-here

# MongoDB connection (handled by Docker Compose)
MONGODB_URI=mongodb://admin:password@mongodb:27017/codersblog?authSource=admin

# Upload directory
UPLOAD_DIR=/uploads

# CORS origin
CORS_ORIGIN=http://localhost:4321